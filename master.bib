	issn = {0042-6989},
	journal = {Vision Research},
	keywords = {Saliency, Vision systems,Visual attention},
	pages = {1489--1506},
	title = {{A saliency-based search mechanism for overt and covert shifts of visual attention}},
	volume = {40},
	year = {2000},
