	isbn = {1529-2401 (Electronic)$\backslash$r0270-6474 (Linking)},
	issn = {0270-6474},
	journal = {Journal of Neuroscience},
	mendeley-groups = {Child recal paper,Feedback/Reinforcement,Affordances Children},
	number = {8},
	pages = {3055--3065},
	pmid = {21414926},
	title = {{Younger Is Not Always Better: Development of Locomotor Adaptation from Childhood to Adulthood}},
	url = {http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.5781-10.2011},
