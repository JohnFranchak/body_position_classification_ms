	isbn = {978-1-4244-6695-5},
	issn = {00220965},
	journal = {Journal of Experimental Child Psychology},
	keywords = {Development,Dynamic,Eye movements,Faces,Gaze,Saliency,Visual attention},
	pages = {293--309},
